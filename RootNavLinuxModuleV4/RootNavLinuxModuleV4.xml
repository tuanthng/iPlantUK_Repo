<!-- 			<template> -->
<!-- 				<tag name="label" value="Curvature Profile" /> -->
<!-- 				<tag name="defaultValue" value="false" type="boolean" /> -->
<!-- 				<tag name="editable" value="true" type="boolean" /> -->
<!-- 				<tag name="description" value="This value will be used for measurement" /> -->
<!-- 			</template> -->
