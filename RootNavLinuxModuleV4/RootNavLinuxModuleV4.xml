		<tag name="CurvatureProfile" value="false" type="combo">
			<template>
				<tag name="label" value="Curvature Profile" />
				<tag name="select">
					<value>true</value>
					<value>false</value>
				</tag>
				<tag name="defaultValue" value="false" />
				<tag name="description" value="This value will be used for measurement" />
			</template>
		</tag>
		
